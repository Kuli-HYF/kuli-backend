{
  "kind": "collectionType",
  "collectionName": "kuli_users",
  "info": {
    "singularName": "kuli-user",
    "pluralName": "kuli-users",
    "displayName": "kuliUser",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string"
    },
    "password": {
      "type": "password",
      "required": true,
      "minLength": 4,
      "maxLength": 20
    },
    "email": {
      "type": "email",
      "required": true
    },
    "isWorking": {
      "type": "boolean",
      "default": false
    },
    "work": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "companies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::company.company",
      "inversedBy": "kuli_users"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "kuli_user"
    },
    "sector": {
      "type": "enumeration",
      "enum": [
        "tech",
        "creative",
        "service",
        "education",
        "finance"
      ]
    },
    "gender": {
      "type": "enumeration",
      "enum": [
        "male",
        "female",
        "other"
      ]
    },
    "lastName": {
      "type": "string"
    }
  }
}
